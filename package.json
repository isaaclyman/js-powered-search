{
  "name": "js-powered-search",
  "displayName": "JS Powered Search",
  "description": "Use the power of JavaScript or TypeScript to search your project",
  "version": "0.0.1",
  "publisher": "Isaac Lyman",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "search",
    "programmatic",
    "project-wide",
    "universal"
  ],
  "preview": true,
  "activationEvents": [
    "onCommand:js-powered-search.scaffold",
    "onCommand:js-powered-search.search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "js-powered-search.scaffold",
        "title": "Scaffold a search definition file",
        "category": "JS Powered Search"
      },
      {
        "command": "js-powered-search.search",
        "title": "Search using this definition file",
        "category": "JS Powered Search"
      }
    ]
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false,
      "description": "This extension executes code in the workspace and runs tasks, which isn't allowed in Restricted Mode."
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.59.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "require-from-string": "^2.0.2"
  }
}
